                                                                    <% if(orders[i].status !== "DELIVERED") { %>
                                                                        <a href="/user/cancel-order/<%= orders[i]._id %>" class="btn-small d-block">Cancel</a>
                                                                    <% } %>
                                                                </td>
                                                            </tr>
                                                                <% } %>
                                                            <% }  %>
                                                        </tbody>
                                                    </table>
